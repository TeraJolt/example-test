{
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": [
    "node_modules"
  ],
  "include": [
    ".plasmo/index.d.ts",
    "./**/*.ts",
    "./**/*.tsx",
  ],
  "compilerOptions": {
    "paths": {
      "~*": [
        "./src/*"
      ],
      "popups*":[
        "./src/popups/*"
      ]
    },
    "baseUrl": ".",
    "types": ["vite/client"]
  }
}
